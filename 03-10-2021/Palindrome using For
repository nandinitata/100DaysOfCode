#include<iostream>
using namespace std;
int main()
{
    int i,n,r,s=0;
    cout<<"Enter The Number:";
    cin>>n;
    for(i=n;i>0; )
    {
        r=i%10;
        s=s*10+r;
        i=i/10;
    }
    if(s==n)
    {
        printf("\n %d is a Palindrome Number",n);
    }
    else
    {
        printf("\n %d is not a Palindrome Number",n);
    }
    
}
