#include<iostream>
#include<conio.h>
 using namespace std;

int main()
{
  int b, oct=0, j=1, remind,octal=0;
  cout<<"Enter the Binary Number: ";
  cin>>b;
  while (b!= 0)
   {
     remind = b%10;
     oct= oct+ remind* j;
     j=j*2;
     b= b/ 10;
   }
   while (oct!= 0)
        {
            remind = oct % 8;
            octal+= remind * i;
            oct/= 8;
            i *= 10;
        }
   cout<<"Octal value: " <<octal;
}
